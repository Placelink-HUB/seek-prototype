<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="biz.placelink.seek.customer.service.CustomerMapper">

    <insert id="insertCustomerInquiries">
        /** insertCustomerInquiries : 고객 문의사항을 등록한다. */
        INSERT INTO SEEK_CUSTOMER_INQUIRIES (
            NAME,
            EMAIL,
            SUBJECT,
            CONTENT,
            CREATE_DT
        ) VALUES (
            #{name},
            #{email},
            #{subject},
            #{content},
            NOW()
        )
    </insert>

</mapper>
