package biz.placelink.aicas.analysis.vo;

import java.time.LocalDate;
import java.time.LocalDateTime;

public class AnalysisRequestVO extends AnalysisParticipantsVO {

    /* 분석 id */
    private String analysisId;
    /* 사용자 식별자 */
    private String userUid;
    /* 대상자 id */
    private String participantsId;
    /* 분석 구분 공통코드 */
    private String analysisSeCcd;
    private String analysisSeCcdNm;
    /* 분석 상태 공통코드 */
    private String analysisStatusCcd;
    private String analysisStatusCcdNm;
    /* 검체 체취 일자 */
    private LocalDate specimenCollectionDe;
    /* 검진 일시 */
    private LocalDateTime checkupDt;

    public String getAnalysisId() {
        return analysisId;
    }

    public void setAnalysisId(String analysisId) {
        this.analysisId = analysisId;
    }

    public String getUserUid() {
        return userUid;
    }

    public void setUserUid(String userUid) {
        this.userUid = userUid;
    }

    public String getParticipantsId() {
        return participantsId;
    }

    public void setParticipantsId(String participantsId) {
        this.participantsId = participantsId;
    }

    public String getAnalysisSeCcd() {
        return analysisSeCcd;
    }

    public void setAnalysisSeCcd(String analysisSeCcd) {
        this.analysisSeCcd = analysisSeCcd;
    }

    public String getAnalysisSeCcdNm() {
        return analysisSeCcdNm;
    }

    public void setAnalysisSeCcdNm(String analysisSeCcdNm) {
        this.analysisSeCcdNm = analysisSeCcdNm;
    }

    public String getAnalysisStatusCcd() {
        return analysisStatusCcd;
    }

    public void setAnalysisStatusCcd(String analysisStatusCcd) {
        this.analysisStatusCcd = analysisStatusCcd;
    }

    public String getAnalysisStatusCcdNm() {
        return analysisStatusCcdNm;
    }

    public void setAnalysisStatusCcdNm(String analysisStatusCcdNm) {
        this.analysisStatusCcdNm = analysisStatusCcdNm;
    }

    public LocalDate getSpecimenCollectionDe() {
        return specimenCollectionDe;
    }

    public void setSpecimenCollectionDe(LocalDate specimenCollectionDe) {
        this.specimenCollectionDe = specimenCollectionDe;
    }

    public LocalDateTime getCheckupDt() {
        return checkupDt;
    }

    public void setCheckupDt(LocalDateTime checkupDt) {
        this.checkupDt = checkupDt;
    }

}